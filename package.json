{
  "name": "vue-image-compare-viewer",
  "description": "Compare before and after images with Vue.js.",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "compare",
    "image",
    "vue",
    "component",
    "composition-api"
