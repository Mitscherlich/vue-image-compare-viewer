    "eslint": "^8.11.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-prism": "^2.2.3",
    "prismjs": "^1.27.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-dts": "^0.9.10",
